{
  "private": true,
  "devDependencies": {
    "awesome-typescript-loader": "^3.1.3",
    "copy-webpack-plugin": "^4.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-adapter-react-helper": "^1.2.3",
    "jest": "^22.1.4",
    "react-hot-loader": "1.3.0",
    "redux-logger": "^3.0.6",
    "source-map-loader": "^0.2.1",
    "ts-jest": "^22.0.2",
    "webpack": "1.13.0",
    "webpack-dev-server": "1.14.1"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "globals": {
      "__DEV__": true,
      "__RCTProfileIsProfiling": false
    },
    "transform": {
      "\\.(ts|tsx|js|jsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/app/__tests__/setup.ts",
    "testRegex": "/__tests__/.*\\.test\\.(jsx|js)$"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/code-mancers/interceptor.git"
  },
  "scripts": {
    "start": "webpack-dev-server --config --hot --progress --colors --inline --content-base dist",
    "build": "./build",
    "watch": "webpack --progress --colors --watch",
    "test": "jest",
    "test -watch": "jest --watch"
  },
  "dependencies": {
    "@types/chrome": "^0.0.63",
    "@types/classnames": "^0.0.32",
    "@types/enzyme": "^3.1.8",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jasmine": "^2.8.6",
    "@types/jest": "^22.1.1",
    "@types/react": "^15.0.23",
    "@types/react-dom": "^15.5.0",
    "@types/react-redux": "^5.0.14",
    "@types/react-table": "^6.7.8",
    "@types/redux-logger": "^3.0.5",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.9",
    "match-sorter": "^2.2.0",
    "react": "^16.2.0",
    "react-chrome-redux": "^1.5.1",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-table": "^6.7.6",
    "redux": "3.5.2",
    "typescript": "^2.3.2"
  }
}
